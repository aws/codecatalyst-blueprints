{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Effect": "Allow",
          "Action": [
              "s3:PutObject",
              "s3:GetObject",
              "iam:PassRole",
              "iam:DeleteRole",
              "iam:GetRole",
              "iam:TagRole",
              "iam:CreateRole",
              "iam:AttachRolePolicy",
              "iam:DetachRolePolicy",
              "cloudformation:*",
              "lambda:*",
              "apigateway:*"
          ],
          "Resource": "*"
      }
  ]
}
